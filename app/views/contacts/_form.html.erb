<%= form_with(url: '/contacts', method: "post") do |f| %>
  <div class="form-group">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>

  <div class="form-group">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <%= f.submit class:'btn btn primary', data: { confirm: 'Create a contact' } %>

<% end %>